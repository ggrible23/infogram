{
	"type": "wordpress-plugin",
	"name": "ggrible/infogram",
	"description": "Embed interactive charts and infographics from Infogram.com",
	"license": "MIT",
	"keywords": [
		"infogram",
		"wordpress",
		"wp"
	],
	"require": {
        "php": ">=8.0",
        "composer/installers": "^1.0 || ^2.0"
	},
    "prefer-stable": true,
	"config": {
		"allow-plugins": {
			"composer/installers": true
		},
		"preferred-install": "dist",
		"sort-packages": true
	}
}
